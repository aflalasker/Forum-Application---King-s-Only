<% @page_title = "Update Category" %>

<div>
    <h2>Update Category</h2>
    
    <%= form_for(:category, :url => {:action => 'update', :id => @category.id}) do |f| %>
    
        <%= render(:partial => "form", :locals => {:f => f}) %>
        
        <div>
            <%= submit_tag("Update") %>
        </div>
    <% end %>
</div>